        $(document).ready(function () {
            // put all your jQuery goodness in here.
            //alert('done');
            $('[filterplaceholder]').focus(function () {
                var input = $(this);
                if (input.val() == input.attr('filterplaceholder')) {
                    input.val('');
                    input.removeClass('filterplaceholder');
                }
            }).blur(function () {
                        var input = $(this);
                        if (input.val() == '' || input.val() == input.attr('filterplaceholder')) {
                            input.addClass('filterplaceholder');
                            input.val(input.attr('filterplaceholder'));
                        }
                        $('.gips-container').css({ display: 'none' });
                    }).blur();

        });

